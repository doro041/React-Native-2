import React, { useState } from 'react';
import { View, Text } from 'react-native';
import { ProgressBar } from 'react-native-progress';

const MySprite = () => {
  const [experience, setExperience] = useState(0);

  return (
    <View>
      <Text>My sprite</Text>
      <ProgressBar
        progress={experience / 100}
        width={200}
        height={20}
      />
    </View>
  );
};

export default MySprite;